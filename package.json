{
  "name": "terminal-macros",
	"displayName": "Terminal Macros",
	"description": "Do anything with the terminal without touching the terminal. Save your file, clear the terminal, and send a custom input to the terminal.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:terminal-macros.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "terminal-macros.helloWorld",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
    }

}
